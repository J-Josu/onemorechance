{
  "name": "onemorechance",
  "version": "0.0.1",
  "private": true,
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:start": "vite dev --host --force",
    "start": "node .",
    "build": "vite build && tsc ./server/sockets.ts --outDir ./build --module nodenext --resolveJsonModule",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:all": "npm run check && npm run check:wss",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:wss": "tsc ./server/sockets.ts --noEmit --module nodenext --resolveJsonModule --skipLibCheck && echo wss check passed âœ… && exit 0",
    "lint": "eslint .",
    "db:drop": "tsx drizzle/drop",
    "db:generate": "npx drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema",
    "db:migrate": "tsx drizzle/migrate",
    "db:seed": "tsx drizzle/seed",
    "db:studio": "npx drizzle-kit studio --config ./drizzle/config.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.56.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.33",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-french-toast": "^1.2.0",
    "svelte-hypercommands": "^0.0.9",
    "sveltekit-superforms": "^2.0.0-alpha.49",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.1",
    "@libsql/client": "^0.4.3",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "nanoid": "^4.0.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  }
}
